        <sys_id>68e31cb51b88a010b40420a8ec4bcb10</sys_id>
        <sys_mod_count>129</sys_mod_count>
        <sys_name>stepGuideTemplate.html</sys_name>
        <sys_package display_value="DemoHub App Home" source="x_snc_dh_app_home">3a61c8fd1b08a010b40420a8ec4bcbbc</sys_package>
        <sys_policy/>
        <sys_scope display_value="DemoHub App Home">3a61c8fd1b08a010b40420a8ec4bcbbc</sys_scope>
        <sys_update_name>sp_ng_template_68e31cb51b88a010b40420a8ec4bcb10</sys_update_name>
