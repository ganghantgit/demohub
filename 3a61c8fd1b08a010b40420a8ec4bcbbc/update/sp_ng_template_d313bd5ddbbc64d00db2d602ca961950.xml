        <template><![CDATA[<div class="content">
  <div class="info-container">
    <ul class="list-group">
      <li ng-if="::c.narrBrief.gsf_version != ''" 
          ng-repeat="item in c.info"
          ng-hide="item.label === 'Demo' || 
                   item.label === 'Prep' ||
                   item.label === 'Install' ||
                   item.label === 'Type' ||
                   item.label === 'Gsf Version' ||
                   item.label === 'Gsf Version Url'"
          ng-show="::item.data"
          class="list-group-item">
        <label>
          {{ ::item.label }}
        </label>
        {{ ::item.data }}
      </li>
      <li ng-if="::c.narrBrief.gsf_version == ''"
          ng-repeat="item in c.info"
          ng-hide="item.label === 'Demo' || 
                   item.label === 'Prep' ||
                   item.label === 'Install' "
          ng-show="::item.data"
          class="list-group-item">
        <label>
          {{ ::item.label }}
        </label>
        {{ ::item.data }}
      </li>
    </ul>
    <ul class="list-group">
      <li ng-repeat="item in c.info"
          ng-if="item.label === 'Demo' || 
                 item.label === 'Prep' ||
                 item.label === 'Install' "
          class="glyphicon glyphicon-time"
          ng-show="::item.data"
          class="list-group-item">
        <label>
          <span class="glyphicon glyphicon-time"
                uib-tooltip="Estimated {{ item.label }} Time">
          </span>
          {{ ::item.label }}
        </label>
        ~{{ ::item.data }}
      </li>
    </ul>                    
  </div>
  <div id="description" 
       ng-bind-html="::c.narrBrief.description"></div>
</div>
<div class="clearfix"></div>]]></template>
    </sp_ng_template>
