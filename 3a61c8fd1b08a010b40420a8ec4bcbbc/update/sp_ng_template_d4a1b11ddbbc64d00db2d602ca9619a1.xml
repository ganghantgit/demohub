        <template><![CDATA[<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th ng-repeat="head in c.tableHead">{{ head }}</th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat="item in c.narrBrief.attachments">
        <td>{{ $index + 1}}</td>
        <td class="col-xs-7">
          <!--<a uib-tooltip="{{ item.file_name }}"
             ng-href="$viewer.do?sysparm_stack=no&sysparm_sys_id= {{ item.sys_id }}" 
             target="_blank">{{ ::item.file_name | limitTo: 45 }}
            {{ ::item.file_name.length > 45 ? '...' : '' }}
