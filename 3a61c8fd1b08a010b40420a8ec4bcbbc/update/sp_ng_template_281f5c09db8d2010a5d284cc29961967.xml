        <template><![CDATA[<div ng-if="c.hasScrolled"
     ng-click="c.toggleFindProcess()">      
  <span ng-show="!c.queryFormOpen && !c.isKeyworkSearch"
        class="selected">
    {{ ::c.querySentence.start }}
    <span>{{ c.currentItem.industry }}</span> 
    {{ ::c.querySentence.intro }}
    <span ng-if="c.data.gsf == true">{{ c.currentItem.gsf_category }}</span> 
    <span ng-if="c.data.gsf == false">{{ c.currentItem.workflow }}</span> 
    {{ ::c.querySentence.workflow_category }}
     <span ng-if="c.data.gsf == true">{{ c.currentItem.persona }}</span>
    {{ ::c.querySentence.persona }}
    <span ng-if="c.data.gsf == true">{{ c.currentItem.gsf_imperative }}</span> 
    <span ng-if="c.data.gsf == false">{{ c.currentItem.businessUnit }}</span> 
    {{ ::c.querySentence.imperative_bu }}
    <span ng-if="c.data.gsf == true">{{ c.currentItem.gsf_capability }}</span> 
    {{ ::c.querySentence.capability }}
    <span>{{ c.currentItem.skillLevel }}</span>
    {{ ::c.querySentence.skillLevel }}
    <sup><i class="icon-view"
            uib-tooltip="Level of product knowledge required to successfully deliver the demo" 
            tooltip-placement="bottom"></i></sup>
  </span>      
  <span ng-show="!c.queryFormOpen && 
                 c.isKeyworkSearch">
    <i ng-class="{ 'glyphicon glyphicon-search' : !c.queryFormOpen }"></i>
    {{ ::c.querySentence.question }}
  </span>
</div>]]></template>
    </sp_ng_template>
</record_update>
